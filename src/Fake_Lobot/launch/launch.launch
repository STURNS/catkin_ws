<launch>

<node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen" clear_params="true">
    <rosparam file="$(find Fake_Lobot)/config/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find Fake_Lobot)/config/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find Fake_Lobot)/config/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find Fake_Lobot)/config/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find Fake_Lobot)/config/base_local_planner_params.yaml" command="load" />
</node>
<node pkg="practic" type="test.py" name="tets" >
    <rosparam file="$(find Fake_Lobot)/config/base_local_planner_params.yaml" command="load" />
</node>  
<node name="map_server" pkg="map_server" type="map_server" args="$(find rbx1_nav)/maps/test_map.yaml"/>
<node pkg="amcl" type="amcl" name="amcl" output="screen" />
<node pkg="Fake_Lobot" type="Fake_Lobot.py" name="Fake_Lobot" />
<node pkg="rviz" type="rviz" name="rviz" />
</launch>